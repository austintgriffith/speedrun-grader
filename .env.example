# Etherscan API v2
ETHERSCAN_API_KEY=

# API Key for authenticated endpoints (required for /install endpoint)
API_KEY=

# PM2 Configuration
# Enable/disable PM2 restart after challenge installation (default: true)
ENABLE_PM2_RESTART=true

# PM2 process name to restart (default: "index")
# Use "all" to restart all processes, or specify a specific process name
PM2_PROCESS_NAME=index

# Delay in milliseconds before restarting PM2 (default: 1000ms)
# This allows the HTTP response to be sent before restart
PM2_RESTART_DELAY=1000
